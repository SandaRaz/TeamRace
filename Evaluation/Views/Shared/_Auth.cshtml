    <link rel="stylesheet" type="text/css" href="~/template/assets/js/select.dataTables.min.css">

    <link rel="stylesheet" href="~/css/docs.css" asp-append-version="true" />
    <link rel="stylesheet" href="~/css/site.css" asp-append-version="true" />
    <link rel="stylesheet" href="~/css/template.css" asp-append-version="true" />
    <link rel="stylesheet" href="~/css/dialog.css" asp-append-version="true" />
    <!-- inject:css -->
    <link rel="stylesheet" href="~/template/assets/css/style.css">

    <!-- endinject -->
    <link rel="shortcut icon" href="~/template/assets/images/favicon.png" />
</head>
<body class="bg-light">
    <header></header>
    <div class="container-scroller">
        <div class="container-fluid page-body-wrapper full-page-wrapper">
            <div class="content-wrapper d-flex align-items-center auth px-0">
                @RenderBody()
            </div>
        </div>
    </div>

    <script src="~/lib/jquery/dist/jquery.min.js"></script>
    <script src="~/js/docs.min.js" asp-append-version="true"></script>
    <script src="~/js/site.js" asp-append-version="true"></script>
